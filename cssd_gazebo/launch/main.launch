<?xml version="1.0"?>
<launch>

  <arg name="gui" default="true" doc="Starts gazebo gui" />

  <!-- startup simulated world -->
  <include file="$(find gazebo_ros)/launch/empty_world.launch">
   <arg name="world_name" default="$(find cssd_gazebo)/worlds/cssd_two_arms.world"/>
    <env name="GAZEBO_MODEL_PATH" value="$(find cssd_gazebo)/models" />
    <arg name="paused" value="true"/>
    <arg name="gui" value="$(arg gui)"/>
  </include>

  <!-- include our robots -->
  <include file="$(find cssd_gazebo)/launch/two_arms.launch" >
    <!-- "ur10" for arm2 / "ur10e" for arm2 -->
    <arg name="mode" value="ur10"/>
  </include>
  
  <!-- trying to remap world -->

</launch>
